{
  "task_id": "inv-regression-001",
  "description": "Dashboard Migration v38 Table Panel Regression",
  "weights": {
    "required_findings": 0.40,
    "file_references": 0.30,
    "causal_chain": 0.20,
    "negative_checks": 0.10
  },
  "required_findings": [
    {
      "id": "f1",
      "description": "Identifies schema version 38",
      "patterns": ["[Vv]38", "schema.?[Vv]ersion.{0,10}38", "schemaVersion.{0,10}38"],
      "weight": 0.25
    },
    {
      "id": "f2",
      "description": "Mentions fieldConfig.defaults.custom",
      "patterns": ["fieldConfig\\.defaults\\.custom", "defaults\\.custom", "defaults\\[.custom.\\]"],
      "weight": 0.25
    },
    {
      "id": "f3",
      "description": "Identifies early continue/return skipping overrides",
      "patterns": ["continue", "early return", "skip.*override", "bypass.*override"],
      "weight": 0.25
    },
    {
      "id": "f4",
      "description": "Mentions migrateOverrides being skipped",
      "patterns": ["migrateOverrides", "migrate.*[Oo]verrides?"],
      "weight": 0.25
    }
  ],
  "file_references": [
    {
      "id": "r1",
      "description": "Identifies v38.go migration file",
      "patterns": ["v38\\.go", "schemaversion/v38", "migration/schemaversion"],
      "weight": 0.33
    },
    {
      "id": "r2",
      "description": "Identifies processPanelsV38 function",
      "patterns": ["processPanelsV38", "processPanels.*V38"],
      "weight": 0.34
    },
    {
      "id": "r3",
      "description": "Identifies migrateOverrides function",
      "patterns": ["migrateOverrides"],
      "weight": 0.33
    }
  ],
  "causal_chain": [
    {
      "id": "c1",
      "description": "Explains nil check on defaults.custom leads to continue which skips migrateOverrides",
      "patterns": ["nil|missing|absent", "continue|skip|bypass", "migrateOverrides|override.*lost"],
      "ordered": true,
      "weight": 0.50
    },
    {
      "id": "c2",
      "description": "Explains why explicit defaults.custom works (type assertion succeeds)",
      "patterns": ["explicit|present|set", "type assertion|assertion succeed|ok.*true|not nil"],
      "ordered": false,
      "weight": 0.50
    }
  ],
  "negative_checks": [
    {
      "id": "n1",
      "description": "Does NOT blame rendering, frontend, or dashboard save logic",
      "patterns": ["rendering.*bug|frontend.*cause|save.*logic.*broken|browser.*issue"],
      "must_be_absent": true,
      "weight": 1.0
    }
  ]
}
